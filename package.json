{
  "name": "vehicle-expiry-notifier",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run build && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit --watch",
    "watch-build": "nodemon --watch src -e ts,tsx,js,jsx --exec \"next build\"",
    "build-iterative": "concurrently \"npm run type-check\" \"npm run watch-build\""
  },
  "dependencies": {
    "date-fns": "^3.3.1",
    "lucide-react": "^0.344.0",
    "next": "13.5.6",
    "nodemailer": "^6.9.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@types/nodemailer": "^6.4.14",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.18",
    "concurrently": "^9.1.2",
    "nodemon": "^3.1.9",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  }
}
